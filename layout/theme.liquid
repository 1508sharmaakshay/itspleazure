		{% comment %} <!-- Power Tools view count code --> {% endcomment %}
		{% if template contains 'product' %}
			<script type="text/javascript">
				(function () {
					var oldonload = window.onload;
					window.onload = function(){
						var scr = document.createElement("script");
						scr.setAttribute('async', 'true');
						scr.type = "text/javascript";
						scr.src = "//ping.powertoolsapp.com/ping/{{ shop.permanent_domain }}/{{ product.id }}?weight={% if product.tags contains 'Featured' or product.tags contains 'featured' %}100{% else %}1{% endif %}{% if collection %}&collection_id={{ collection.handle }}{% endif %}";
						((document.getElementsByTagName('head') || [null])[0] || document.getElementsByTagName('script')[0].parentNode).appendChild(scr);
						if(oldonload){oldonload()}
                    };
				});
			</script>
		{% endif %}
